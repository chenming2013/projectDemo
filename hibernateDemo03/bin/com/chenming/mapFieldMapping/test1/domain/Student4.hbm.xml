<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
	
<!-- 该文件用来配置entity对象和数据库表的映射关系 -->
<!-- package指明要映射到数据库中的类在哪个包下 -->
<hibernate-mapping package="com.chenming.mapFieldMapping.test1.domain">
	<class name="com.chenming.mapFieldMapping.test1.domain.Student4" table="t_student4" lazy="false">

		<id name="id" column="id" type="string">
			<generator class="uuid"></generator>
		</id>
	
		<property name="cardId" column="card_id" type="string"></property>
		<property name="name" column="name" type="string"></property>
		<property name="age" column="age" type="int"></property>
		
		<set name="">
			
		</set>		
		
	</class>
</hibernate-mapping>