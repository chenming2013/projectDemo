package basic.test;

import org.springframework.context.ApplicationContext;
import org.springframework.context.support.ClassPathXmlApplicationContext;

public class Test {

	@org.junit.Test
	public void method() {
		
		ApplicationContext context = new ClassPathXmlApplicationContext("basic/test/beans.xml");
		
		A a1 = context.getBean("a1",A.class);
		
		A a2 = context.getBean("a2",A.class);
	
		System.out.println(a1.getB()==a2.getB());
				
		System.out.println(a1.getC()==a2.getC());
	}
	
}
