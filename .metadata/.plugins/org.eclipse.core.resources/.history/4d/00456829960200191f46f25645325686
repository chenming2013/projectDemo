package basic.basic_06_2_aop_adviceBaseSchema.aop;

import org.aspectj.lang.ProceedingJoinPoint;

/**
 * 后置最终通知 实现方法定义的类
 * @author 陈明
 * @date 2018年12月18日
 */
public class HelloWorldAfterFinallyAspect {

	public void afterFinallyAdvice1(ProceedingJoinPoint jp,Integer num) throws Throwable {
		if(num>0) {
			jp.proceed();
		}
	}
	
}
